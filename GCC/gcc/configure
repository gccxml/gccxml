.p2align 2' > conftest.s
    if { ac_try='$gcc_cv_as  -o conftest.o conftest.s >&5'
  { (eval echo "$as_me:$LINENO: \"$ac_try\"") >&5
  (eval $ac_try) 2>&5
  ac_status=$?
  echo "$as_me:$LINENO: \$? = $ac_status" >&5
